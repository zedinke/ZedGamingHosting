{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "validate-locales": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run validate:locales"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/apps/web/.next"],
      "options": {
        "cwd": "apps/web",
        "command": "node build-wrapper.js"
      },
      "configurations": {
        "production": {}
      }
    }
  }
}
